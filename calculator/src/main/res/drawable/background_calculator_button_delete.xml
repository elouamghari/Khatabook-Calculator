<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <solid
                android:color="@color/default_button_delete_pressed_color"/>
            <corners
                android:radius="@dimen/default_button_radius"/>
            <stroke
                android:width="@dimen/default_button_stroke_width"
                android:color="@color/default_button_delete_stroke_color"/>
        </shape>
    </item>
    <item>
        <shape>
            <solid
                android:color="@color/default_button_delete_default_color"/>
            <corners
                android:radius="@dimen/default_button_radius"/>
            <stroke
                android:width="@dimen/default_button_stroke_width"
                android:color="@color/default_button_delete_stroke_color"/>
        </shape>
    </item>
</selector>